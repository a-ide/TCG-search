{
  "name": "tcg-search",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:fix": "npm run lint:eslint -- --fix && npm run lint:stylelint -- --fix",
    "lint:eslint": "eslint src/**/*.{js,jsx}",
    "lint:stylelint": "node_modules/stylelint/bin/stylelint.js src/**/*.scss",
    "clean:img": "rm -rf ./dist/img && mkdir ./dist/img",
    "make:file": "rm -rf dist && mkdir -p dist/data dist-scripts/data dist-scripts/img && touch dist/data/catList.json dist-scripts/data/catList.json",
    "build": "npm run build:back && npm run build:front",
    "build:back": "npm run make:file && babel --config-file ./babel-back.config.js scripts --out-dir dist-scripts && node ./dist-scripts/index.js && mv dist-scripts/data/catList.json dist/data/catList.json && mv dist-scripts/img dist && rm -rf dist-scripts",
    "build:front": "npx webpack --config ./webpack-front.config.js",
    "start": "webpack-dev-server --config ./webpack-front.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-ide/TCG-search.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/a-ide/TCG-search/issues"
  },
  "homepage": "https://github.com/a-ide/TCG-search#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^1.0.1",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "node-sass": "^4.14.1",
    "postcss-loader": "^4.0.1",
    "prettier": "2.1.1",
    "prettier-stylelint": "^0.4.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "autoprefixer": "^9.8.6",
    "axios": "^0.20.0",
    "deep-object-diff": "^1.1.0",
    "delay": "^4.4.0",
    "html-webpack-plugin": "^4.4.1",
    "i": "^0.3.7",
    "npm": "^6.14.8",
    "puppeteer": "^5.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "reset-css": "^5.0.1",
    "webpack-node-externals": "^2.5.2"
