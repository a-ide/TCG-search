{
  "name": "tcg-search",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:fix": "npm run lint:eslint -- --fix && npm run lint:stylelint -- --fix",
    "lint:eslint": "eslint src/**/*.{js,jsx}",
    "lint:stylelint": "node_modules/stylelint/bin/stylelint.js src/**/*.scss",
    "clean:img": "rm -rf ./dist/img && mkdir ./dist/img",
    "make:file": "rm -rf dist && mkdir -p dist/data dist-scripts/data dist-scripts/img && touch dist/data/catList.json dist-scripts/data/catList.json",
    "build": "npm run build:back && npm run build:front",
    "build:back": "npm run make:file && babel --config-file ./babel-back.config.js scripts --out-dir dist-scripts && node ./dist-scripts/index.js && mv dist-scripts/data/catList.json dist/data/catList.json && mv dist-scripts/img dist && rm -rf dist-scripts",
    "build:front": "npx webpack --config ./webpack-front.config.js",
    "start": "webpack-dev-server --config ./webpack-front.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-ide/TCG-search.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/a-ide/TCG-search/issues"
  },
  "homepage": "https://github.com/a-ide/TCG-search#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "prettier": "1.14.3",
    "prettier-stylelint": "^0.4.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.3.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "autoprefixer": "^9.3.1",
    "axios": "^0.18.0",
    "deep-object-diff": "^1.1.0",
    "delay": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "i": "^0.3.6",
    "npm": "^6.4.1",
    "puppeteer": "^1.9.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "reset-css": "^4.0.1",
    "webpack-node-externals": "^1.7.2"
